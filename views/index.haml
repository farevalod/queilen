!!! 5
%html(lang="en")
  %head
    %title Queilen Nativa
    %script(type="text/javascript" src="//use.typekit.net/hyl7ure.js")
    %script(type="text/javascript")
      try{Typekit.load();}catch(e){}
    %link{:rel => 'stylesheet', :href => 'bootstrap/css/bootstrap.css', :type => "text/css"}
    %link{:rel => 'stylesheet', :href => 'css/styles.css', :type => "text/css"}
  %body
    .topbar
      .fill
        .container
          =haml :header
    .container#main-box
      .row.spacer
        .span11.texto-foto#papa-nativa
          %span.center-pad Descubre la papa nativa chilota
      .row.spacer
        .span3.texto-foto#papa-lover
          %span.text-pad Papa lover?
        .span4.texto-foto#comidas-tipicas
          %span.text-pad Comidas tipicas
        .span4.texto-foto#la-bruja
          %span.text-pad Te has comido la bruja?
      .row.spacer
        .span8.texto-foto#huerto-casero
          %span.center-pad Huerto casero criollo
        .span3.texto-foto#variedades
      %hr
      .footer
        %p &copy; Queilen Nativa 2013
